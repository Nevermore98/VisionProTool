<UserControl x:Class="VisionPro_Tool.Views.Pages.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cogtool="clr-namespace:Cognex.VisionPro.ToolBlock;assembly=Cognex.VisionPro.ToolGroup.Controls"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:VisionPro_Tool.Views.Pages"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:pages="clr-namespace:VisionPro_Tool.ViewModels.Pages"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             d:DataContext="{d:DesignInstance Type=pages:SettingsPageVM,
                                              IsDesignTimeCreatable=True}"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <Button Content="hi" />
        <ui:Button Appearance="Danger" Content="hi" Foreground="White" />
        <ui:Button Appearance="Success" Content="hi" Foreground="White" />
        <ui:Button Appearance="Primary" Content="WPF UI button with font icon" Icon="{ui:FontIcon '🌈'}" />
        <ui:Button Appearance="Primary" Content="WPF UI button with font icon" Icon="{ui:SymbolIcon Symbol=Fluent24}" />
    </StackPanel>
</UserControl>
